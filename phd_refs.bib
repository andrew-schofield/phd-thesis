@ARTICLE{Alexander2004,
  author = {Alexander, Heather L. and Rasmussen, Andrew W. and Stojiljkovic,
	Igor},
  title = {Identification of {Neisseria meningitidis} Genetic Loci Involved
	in the Modulation of Phase Variation Frequencies},
  journal = {Infect. Immun.},
  year = {2004},
  volume = {72},
  pages = {6743-6747},
  number = {11},
  abstract = {It has been proposed that increased phase variation frequencies in
	Neisseria meningitidis augment transmissibility and invasiveness.
	A Himar1 mariner transposon mutant library was constructed in serogroup
	A N. meningitidis and screened for clones with increased phase variation
	frequencies. Insertions increasing the frequency of slippage events
	within mononucleotide repeat tracts were identified in three known
	phase variation-modulating genes (mutS, mutL, and uvrD), as well
	as six additional loci (pilP, fbpA, fbpB, NMA1233, and two intergenic
	regions). The implications of these insertion mutations are discussed.},
  doi = {10.1128/IAI.72.11.6743-6747.2004},
  eprint = {http://iai.asm.org/cgi/reprint/72/11/6743.pdf},
  file = {:Identification of Neisseria meningitidis genetic loci involved in the modulation of phase variation frequencies.pdf:PDF},
  owner = {uncle_fungus},
  timestamp = {2011.07.12},
  url = {http://iai.asm.org/cgi/content/abstract/72/11/6743}
}

@ARTICLE{almeida_unifying_1997,
  author = {J. S. Almeida and M. A. M. Reis and M. J. T. Carrondo},
  title = {A Unifying Kinetic Model of Denitrification},
  journal = {Journal of Theoretical Biology},
  year = {1997},
  volume = {186},
  pages = {241--249},
  number = {2},
  month = may,
  abstract = {A novel unifying kinetic model of the sequential reduction of nitrogen
	oxides by denitrifying microorganisms is proposed. Its mathematical
	structure is based on an analogy between the organization of the
	cell membrane electron transport chain and an electric circuit. The
	resulting kinetic model is simple enough to be of practical use as
	both an interpretative and a predictive tool. Furthermore, previous
	attempts to develop unifying models of dentrification (predicting
	the accumulation of any number of intermediates) are briefly reviewed
	and compared with the one hereby proposed. The model is validated
	with experimental data reported elsewhere, for nitrite accumulation
	{byPseudomonas} fluorescensand nitrite and nitrous oxide accumulation
	{byParacoccus} dentrificans. Author Keywords: ; f1 Author to whom
	correspondence should be addressed.},
  doi = {10.1006/jtbi.1996.0352},
  file = {:A unifying kinetic model of denitrification.pdf:PDF},
